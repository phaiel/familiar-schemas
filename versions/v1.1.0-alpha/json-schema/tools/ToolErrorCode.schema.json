    "INVALID_INPUT": "InvalidInput",
    "LLM_ERROR": "LlmError",
    "MISSING_PARAMETER": "MissingParameter",
    "NOT_FOUND": "NotFound",
    "PERMISSION_DENIED": "PermissionDenied",
    "RATE_LIMITED": "RateLimited",
    "SERVICE_UNAVAILABLE": "ServiceUnavailable",
    "TIMEOUT": "Timeout",
    "VALIDATION_FAILED": "ValidationFailed"
