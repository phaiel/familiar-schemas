{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Error codes for tool failures",
  "oneOf": [
    {
      "description": "Invalid input parameters",
      "enum": [
        "INVALID_INPUT"
      ],
      "type": "string"
    },
    {
      "description": "Missing required parameter",
      "enum": [
        "MISSING_PARAMETER"
      ],
      "type": "string"
    },
    {
      "description": "Parameter validation failed",
      "enum": [
        "VALIDATION_FAILED"
      ],
      "type": "string"
