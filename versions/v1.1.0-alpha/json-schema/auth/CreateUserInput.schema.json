{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Input for creating a new user",
  "properties": {
    "avatar_url": {
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "email": {
      "$ref": "../primitives/Email.schema.json"
    },
    "name": {
      "type": "string"
    },
    "password_hash": {
      "anyOf": [
        {
          "$ref": "../primitives/PasswordHash.schema.json"
        },
        {
          "type": "null"
        }
      ]
    },
    "primary_tenant_id": {
      "default": null,
      "format": "uuid",
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "email",
    "name"
  ],
  "title": "CreateUserInput",
  "type": "object",
  "x-familiar-kind": "auth",
  "x-familiar-meta-schema": {
    "$ref": "../ecs/Auth.meta.schema.json"
  },
  "x-familiar-requires-auth": true,
  "x-familiar-service": {
    "$ref": "../nodes/familiar-daemon.node.json"
  }
}
