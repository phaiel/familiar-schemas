{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MagicLinkFlowInput",
  "description": "Input for the magic link Windmill flow",
  "type": "object",
  "properties": {
    "action": {
      "$ref": "./MagicLinkAction.schema.json"
    },
    "email": {
      "type": ["string", "null"],
      "format": "email"
    },
    "token": {
      "type": ["string", "null"]
    },
    "name": {
      "type": ["string", "null"]
    },
    "invite_code": {
      "type": ["string", "null"]
    },
    "request_id": {
      "type": "string"
    },
    "ip_address": {
      "type": ["string", "null"]
    },
    "user_agent": {
      "type": ["string", "null"]
    }
  },
  "required": ["action", "request_id"],
  "x-familiar-kind": "request"
}

