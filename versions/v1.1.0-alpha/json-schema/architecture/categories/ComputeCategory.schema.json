{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://familiar.dev/schemas/architecture/architecture/categories/ComputeCategory.schema.json",
  "title": "Compute Category",
  "description": "Computational nature of an action for optimization and scheduling",
  "type": "string",
  "enum": [
    "pure",
    "io",
    "ai",
    "state"
  ],
  "x-familiar-kind": "enum",
  "x-familiar-enum-values": {
    "pure": "Pure computation, no side effects (deterministic)",
    "io": "External I/O operations (network, disk, databases)",
    "ai": "LLM/ML operations (non-deterministic)",
    "state": "State mutations (database writes, cache updates)"
  }
}
