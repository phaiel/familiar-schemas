{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A member of a tenant (user-tenant association with role)\n\nNote: Uses UserId as the entity ID since this is a join table where the user_id + tenant_id forms the unique key.",
  "properties": {
    "avatar_url": {
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "created_at": {
      "format": "date-time",
      "type": "string"
    },
    "email": {
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "id": {
      "$ref": "../primitives/TenantId.schema.json"
    },
    "name": {
      "type": "string"
    },
    "role": {
      "$ref": "../types/MemberRole.schema.json"
    },
    "settings": {
      "default": null
    },
    "tenant_id": {
      "$ref": "../primitives/TenantId.schema.json"
    },
    "updated_at": {
      "format": "date-time",
      "type": "string"
    }
  },
  "required": [
    "created_at",
    "id",
    "name",
    "role",
    "tenant_id",
    "updated_at"
  ],
  "title": "TenantMember",
  "type": "object",
  "x-familiar-kind": "type"
}