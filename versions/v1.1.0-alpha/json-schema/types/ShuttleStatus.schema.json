{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Processing status for a Shuttle",
  "oneOf": [
    {
      "description": "Segment received, not yet processed",
      "enum": [
        "pending"
      ],
      "type": "string"
    },
    {
      "description": "Currently being classified by LLM",
      "enum": [
        "classifying"
      ],
      "type": "string"
    },
    {
      "description": "Classification complete, spawning entities",
      "enum": [
        "spawning"
      ],
      "type": "string"
    },
    {
      "description": "All processing complete",
      "enum": [
        "complete"
      ],
      "type": "string"
    },
    {
      "description": "Processing failed",
      "enum": [
        "failed"
      ],
      "type": "string"
    }
  ],
  "title": "ShuttleStatus",
  "x-familiar-casing": "snake_case",
  "x-familiar-enum-repr": "simple_enum",
  "x-familiar-kind": "type",
  "x-familiar-variants": {
    "classifying": "Classifying",
    "complete": "Complete",
    "failed": "Failed",
    "pending": "Pending",
    "spawning": "Spawning"
  }
}