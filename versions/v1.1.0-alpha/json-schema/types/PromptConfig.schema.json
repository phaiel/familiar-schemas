{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Schema for TypeScript prompt configuration Runtime prompts are in servic../domains/windmill/scripts/*.ts",
  "properties": {
    "max_tokens": {
      "format": "uint32",
      "minimum": 0.0,
      "type": "integer"
    },
    "model": {
      "type": "string"
    },
    "phase": {
      "$ref": "PromptPhase.schema.json"
    },
    "temperature": {
      "format": "float",
      "type": "number"
    }
  },
  "required": [
    "max_tokens",
    "model",
    "phase",
    "temperature"
  ],
  "title": "PromptConfig",
  "type": "object",
  "x-familiar-kind": "type"
}