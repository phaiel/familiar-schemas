{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://familiar.dev/schemas/ecs/ExecutionModel.schema.json",
  "title": "Execution Model",
  "description": "Execution pattern of an action for concurrency and resource management",
  "type": "string",
  "enum": [
    "sync",
    "async",
    "stream",
    "batch"
  ],
  "x-familiar-kind": "enum",
  "x-familiar-enum-values": {
    "sync": "Synchronous execution (blocking)",
    "async": "Asynchronous execution (non-blocking)",
    "stream": "Streaming operations (real-time data)",
    "batch": "Bulk operations (vectorized processing)"
  }
}
