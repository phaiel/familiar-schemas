{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Metadata about an AI completion response",
  "properties": {
    "latency_ms": {
      "format": "uint64",
      "minimum": 0.0,
      "type": "integer"
    },
    "model_id": {
      "type": "string"
    },
    "model_name": {
      "type": "string"
    },
    "provider": {
      "$ref": "../primitives/AIProvider.schema.json"
    },
    "request_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "usage": {
      "anyOf": [
        {
          "$ref": "../primitives/TokenUsage.schema.json"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "latency_ms",
    "model_id",
    "model_name",
    "provider"
  ],
  "title": "ResponseMetadata",
  "type": "object",
  "x-familiar-kind": "component",
  "x-familiar-meta-schema": {
    "$ref": "../ecs/Component.meta.schema.json"
  },
  "x-familiar-service": {
    "$ref": "../nodes/familiar-daemon.node.json"
  }
}