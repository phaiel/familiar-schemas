{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SignupFlowInput",
  "description": "Input for the signup Windmill flow",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": ["string", "null"]
    },
    "name": {
      "type": "string"
    },
    "invite_code": {
      "type": ["string", "null"]
    },
    "consents": {
      "$ref": "../contracts/SignupConsents.schema.json"
    },
    "request_id": {
      "type": "string"
    },
    "ip_address": {
      "type": ["string", "null"]
    },
    "user_agent": {
      "type": ["string", "null"]
    }
  },
  "required": ["email", "name", "consents", "request_id"],
  "x-familiar-kind": "request"
}


