{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "InviteType": {
      "enum": [
        "Email",
        "Code"
      ],
      "type": "string"
    }
  },
  "properties": {
    "created_at": {
      "format": "date-time",
      "type": "string"
    },
    "email": {
      "anyOf": [
        {
          "$ref": "../primitives/Email.schema.json"
        },
        {
          "type": "null"
        }
      ]
    },
    "expires_at": {
      "format": "date-time",
      "type": [
        "string",
        "null"
      ]
    },
    "id": {
      "$ref": "../primitives/InvitationId.schema.json"
    },
    "invite_code": {
      "anyOf": [
        {
          "$ref": "../primitives/InviteCode.schema.json"
        },
        {
          "type": "null"
        }
      ]
    },
    "invite_type": {
      "$ref": "../domains/auth/InviteType.schema.json"
    },
    "invited_by": {
      "anyOf": [
        {
          "$ref": "../primitives/UserId.schema.json"
