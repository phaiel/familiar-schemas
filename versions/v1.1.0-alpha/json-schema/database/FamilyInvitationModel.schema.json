          "type": "null"
        }
      ]
    },
    "max_uses": {
      "format": "int32",
      "type": [
        "integer",
        "null"
      ]
    },
    "role": {
      "$ref": "../primitives/InviteRole.schema.json"
    },
    "tenant_id": {
      "$ref": "../primitives/TenantId.schema.json"
    },
    "use_count": {
      "format": "int32",
      "type": "integer"
    }
  },
  "required": [
    "created_at",
    "id",
    "invite_type",
    "role",
    "tenant_id",
    "use_count"
  ],
  "title": "Model",
  "type": "object",
  "x-familiar-kind": "database",
  "x-familiar-meta-schema": {
    "$ref": "../ecs/Database.meta.schema.json"
  },
  "x-familiar-service": {
    "$ref": "../nodes/familiar-daemon.node.json"
  },
  "x-familiar-table": "family_invitations",
  "x-familiar-module": "auth",
  "x-familiar-primary-key": "id",
  "x-familiar-relations": [
    {
      "type": "belongs_to",
