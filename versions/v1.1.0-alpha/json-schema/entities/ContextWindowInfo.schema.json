{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ContextWindowInfo",
  "description": "Summary of context window usage",
  "type": "object",
  "properties": {
    "total_messages": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "total_tokens": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "max_tokens": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "messages_included": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "tokens_used": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0
    },
    "messages": {
      "type": "array",
      "items": {
        "$ref": "./MessageTokenInfo.schema.json"
      }
    }
  },
  "required": ["total_messages", "total_tokens", "max_tokens", "messages_included", "tokens_used", "messages"],
  "x-familiar-kind": "component"
}


