{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "description": "A user's identity that can belong to multiple families\n\nUses `SystemEntityMeta` because users are not tenant-scoped - a single user can belong to multiple families.",
  "type": "object",
  "required": [
    "created_at",
    "email",
    "email_verified",
    "id",
    "name",
    "updated_at"
  ],
  "properties": {
    "avatar_url": {
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "deletion_requested_at": {
      "default": null,
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "email": {
      "type": "string"
    },
    "email_verified": {
      "type": "boolean"
    },
    "gdpr_consents": {
      "default": null
    },
    "id": {
      "description": "Unique identifier for this entity",
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string"
    },
    "primary_tenant_id": {
      "description": "The user's primary family (can be changed)",
      "default": null,
      "type": [
        "string",
        "null"
      ],
      "format": "uuid"
    },
    "settings": {
      "default": null
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}