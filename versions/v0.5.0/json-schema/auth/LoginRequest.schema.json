{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LoginRequest",
  "description": "Request for email+password login",
  "type": "object",
  "required": [
    "email",
    "password"
  ],
  "properties": {
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  }
}