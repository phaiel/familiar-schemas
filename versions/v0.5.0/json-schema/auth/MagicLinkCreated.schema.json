{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MagicLinkCreated",
  "description": "Result of creating a magic link (includes raw token)",
  "type": "object",
  "required": [
    "expires_at",
    "link_id",
    "token"
  ],
  "properties": {
    "expires_at": {
      "type": "string",
      "format": "date-time"
    },
    "link_id": {
      "type": "string",
      "format": "uuid"
    },
    "token": {
      "type": "string"
    }
  }
}