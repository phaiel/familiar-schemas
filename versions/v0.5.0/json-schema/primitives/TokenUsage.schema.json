{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TokenUsage",
  "description": "Token usage statistics from a completion",
  "type": "object",
  "required": [
    "completion_tokens",
    "prompt_tokens",
    "total_tokens"
  ],
  "properties": {
    "completion_tokens": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "prompt_tokens": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "total_tokens": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}