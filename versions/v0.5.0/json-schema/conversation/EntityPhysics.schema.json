{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EntityPhysics",
  "description": "Physics state for an entity",
  "type": "object",
  "required": [
    "arousal",
    "epistemic",
    "significance",
    "valence"
  ],
  "properties": {
    "arousal": {
      "type": "number",
      "format": "double"
    },
    "epistemic": {
      "type": "number",
      "format": "double"
    },
    "significance": {
      "type": "number",
      "format": "double"
    },
    "valence": {
      "type": "number",
      "format": "double"
    }
  }
}