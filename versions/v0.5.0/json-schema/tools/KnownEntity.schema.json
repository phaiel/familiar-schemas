{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "KnownEntity",
  "description": "Known entity for reference resolution",
  "type": "object",
  "required": [
    "entity_type",
    "id",
    "name"
  ],
  "properties": {
    "aliases": {
      "default": [],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "entity_type": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    }
  }
}